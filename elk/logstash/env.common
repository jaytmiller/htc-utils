#! /bin/sh -eu

# Name of running logstash container
HTC_LOGSTASH_CONTAINER_NAME=${HTC_EVENTLOG_ROOT_NAME}-logstash-${HTC_CONDOR_MACHINE}

# Name of generic Docker image for EventLog logstash
HTC_IMAGE_NAME=${HTC_EVENTLOG_ROOT_NAME}-logstash

# Index pattern in config .json
HTC_EVENTLOG_INDEX_PATTERN=${HTC_EVENTLOG_ROOT_NAME}'-*'

# Name of the elasticsearch index these events will be written to
HTC_EVENTLOG_INDEX_NAME="${HTC_EVENTLOG_ROOT_NAME}-${HTC_CONDOR_MACHINE}-"'%{+YYYY.MM.dd}'

