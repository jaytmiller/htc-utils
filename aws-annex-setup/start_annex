#! /bin/bash

COUNT=${1:-1}
INSTANCE=${2:-t2.micro}
DURATION=${3:-8}
IDLE=${4:-$DURATION}

WORKER_AMI="ami-076c79d19eb4a7d37"

condor_annex -annex-name MyFirstAnnex -duration $DURATION  -idle $IDLE  -count $COUNT -owner ec2-user  -aws-on-demand-instance-type $INSTANCE  -aws-on-demand-ami-id $WORKER_AMI


